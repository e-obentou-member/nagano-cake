<h2>
  <%= customer.first_name %>
  <%= customer.last_name %>
  さんの会員情報
</h2>

<table>
  <% @customers.each do |customer| %>
    <tr>
      <td>会員ID</td>
      <td><%= "#{@id}" %></td>
    </tr>
    <tr>
      <td>氏名</td>
      <td>
        <%= customer.first_name %>
        <%= customer.last_name %>
      </td>
    </tr>
    <tr>
      <td>フリガナ</td>
      <td>
        <%= customer.first_name_kana %>
        <%= customer.last_name_kana %>
      </td>
    </tr>
    <tr>
      <td>郵便番号</td>
      <td><%=%></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%=%></td>
    </tr>
    <tr>
      <td>電話番号</td>
      <td><%=%></td>
    </tr>
    <tr>
      <td>メールアドレス</td>
      <td><%=%></td>
    </tr>
    <tr>
      <td>会員ステータス</td>
      <td><%=%></td>
    </tr>
  <% end %>
</table>
<!--会員情報-->
