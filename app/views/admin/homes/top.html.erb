<h2>注文履歴一覧</h2>

<table>
  <thead>
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <!--できれば注文ステータス-->
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.created_at %></td>
        <td>購入者</td>
        <td>注文個数</td>
        <!--できれば注文ステータス-->
      </tr>
    <% end %>
  </tbody>
</table>

